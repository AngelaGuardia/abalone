<h1>Processed File</h1>
<br />
<%= @processed_file.original_filename %> processed at <%= @processed_file.updated_at %><br />
<br />
<table border="1" style="width:100%;">
  <thead>
  <%- @headers.each do |attr|%>
    <th><%= attr %></th>
  <%- end %>
  </thead>
  <tbody>
  <%- @records.each do |record| %>
    <tr>
      <%- @headers.each do |attr|%>
        <td><%= record[attr] %></td>
      <%- end %>
    </tr>
  <% end %>
  </tbody>
</table>
